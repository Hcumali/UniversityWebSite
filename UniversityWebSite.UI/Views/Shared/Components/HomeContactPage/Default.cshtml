@model Contact

<div class="contentIn">
    <div>
        <h1>Anasayfa - İletişim Bölümü</h1>

        <div class="form-group">
            <label>Oluşturulma Tarihi</label>
            <input type="text" class="form-control" value="@Model.CreatedTime" readonly>
        </div>

        <div class="form-group">
            <label>Güncellenme Tarihi</label>
            <input type="text" class="form-control" value="@Model.UpdatedTime" readonly>
        </div>

        <div class="form-group">
            <label>E-mail</label>
            <input type="text" class="form-control" value="@Model.Email" required>
        </div>

        <div class="form-group">
            <label>Telefon Numarası</label>
            <input type="text" class="form-control" value="@Model.Phone" required>
        </div>

        <div class="form-group">
            <label>Faks Numarası</label>
            <input type="text" class="form-control" value="@Model.Fax" required>
        </div>


        <div class="row justify-content-center">
            <button type="submit" onclick="sendForm()" class="btn btn-primary align-baseline">Güncelle</button>
        </div>

        <div id="error"></div>
    </div>
</div>

<script>
    function sendForm() {
        console.log("working!");
    }
</script>